{
    "name": "TestFairy - Copy app url",
    "description": "Copy the app url (IPA/APK) by one click, from the TestFairy download page",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts":[{
      "matches":["http://my.testfairy.com/*", "https://my.testfairy.com/*"],
      "js":["inject.js"],
      "run_at": "document_end"
    }],
    "permissions": [],
    "action": {
      "default_icon": {
        "16": "/images/testfairy-icon-256.png",
        "32": "/images/testfairy-icon-256.png",
        "48": "/images/testfairy-icon-256.png",
        "128": "/images/testfairy-icon-256.png"
      }
    },
    "icons": {
      "16": "/images/testfairy-icon-256.png",
      "32": "/images/testfairy-icon-256.png",
      "48": "/images/testfairy-icon-256.png",
      "128": "/images/testfairy-icon-256.png"
    }
  }